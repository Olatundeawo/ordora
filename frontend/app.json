{
  "expo": {
    "name": "Ordora",
    "slug": "Ordora",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.3",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "backgroundColor": "#232323",
      "resizeMode": "contain"
    },
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.olatundeawo.ordora",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.olatundeawo.ordora"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bea9bc36-8775-441d-9350-569a283a38c0"
      }
    },
    "owner": "olatundeawo",
    "updates": {
      "url": "https://u.expo.dev/bea9bc36-8775-441d-9350-569a283a38c0"
    }
  }
}
